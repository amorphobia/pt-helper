{
  "name": "userscript-typescript-template",
  "version": "1.1.0",
  "description": "Template repo using Webpack and TypeScript to build your userscript for Tampermonkey and more extensions.",
  "main": "userscript/index.user.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "webpack",
    "build:watch": "webpack --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pboymt/userscript-typescript-template.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pboymt/userscript-typescript-template/issues"
  },
  "homepage": "https://github.com/pboymt/userscript-typescript-template#readme",
  "devDependencies": {
    "@types/node": "^16.11.10",
    "@types/prettier": "^2.7.0",
    "@types/tampermonkey": "^4.0.5",
    "@types/webpack": "^5.28.0",
    "axios-userscript-adapter": "^0.1.12",
    "prettier": "^2.7.1",
    "terser-webpack-plugin": "^5.3.6",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2",
    "webpack": "^5.64.3",
    "webpack-cli": "^4.9.1"
  },
  "userscript": {
    "require-template": "https://cdn.jsdelivr.net/npm/${dependencyName}@${dependencyVersion}",
    "name": "PT Helper",
    "i18n-names": [
      { "i18n": "zh-CN", "name": "PT 助手" }
    ],
    "namespace": "https://github.com/amorphobia/pt-helper",
    "description": "A helper for private trackers",
    "i18n-descriptions": [
      { "i18n": "zh-CN", "description": "私密种子站点的助手" }
    ],
    "version": "0.1.7",
    "author": "amorphobia",
    "homepage": "https://github.com/amorphobia/pt-helper",
    "icon": "data:image/svg+xml;base64,",
    "supportURL": "https://github.com/amorphobia/pt-helper/issues",
    "license": "AGPL-3.0-or-later",
    "match": [
      "*://hhanclub.top/*",
      "*://nanyangpt.com/*",
      "*://pt.sjtu.edu.cn/*",
      "*://pterclub.com/*",
      "*://tjupt.org/*"
    ],
    "connect": [],
    "require": [],
    "grant": [
      "GM_addStyle",
      "GM_registerMenuCommand",
      "GM_unregisterMenuCommand",
      "GM_openInTab",
      "GM_getValue",
      "GM_setValue",
      "GM_notification",
      "GM_xmlhttpRequest"
    ],
    "exclude": [],
    "resources": [],
    "noframes": true
  },
  "dependencies": {
    "sweetalert2": "11.4.8",
    "clipboard": "^2.0.11"
  }
}
